import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        Set<Integer> uniqueNotes = new HashSet<>();
        for (int i = 0; i < n; i++) {
            uniqueNotes.add(sc.nextInt());
        }
        System.out.println(uniqueNotes.size());
    }
}
